{
   "name": "tin",
   "version": "1.0.0",
   "description": "Custom language, procedural, statically-typed, indentation-based",
   "main": "./tin-compiler-ts/compiler.ts",
   "type": "module",
   "scripts": {
      "oldDev": " npx tsx --watch tin-compiler-ts/compiler.ts test",
      "dev": "nodemon --quiet --watch ./tin-compiler-ts/ --watch ./src/**/* --ext js,tin,ts --exec npx --trace-uncaught tsx ./tin-compiler-ts/compiler.ts main --run",
      "devGo": "nodemon --quiet --watch ./tin-compiler-ts/ --watch ./src/**/* --ext js,tin,ts --exec npx --trace-uncaught tsx ./tin-compiler-ts/compiler.ts main --run --targetLanguage:go",
      "devVerbose": "nodemon --quiet --watch ./tin-compiler-ts/ --watch ./src/**/* --ext js,tin,ts --exec \"npx tsx ./tin-compiler-ts/compiler.ts main --run --verbose\"",
      "test": "npx tsx ./tin-compiler-ts/compiler.ts --test"
   },
   "author": "Lumintorious",
   "license": "ISC",
   "devDependencies": {
      "@types/node": "^22.7.6",
      "tsx": "^4.19.1"
   }
}
