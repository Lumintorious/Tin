Person = type
	name :: String
	house :: String
	title :: String

robert = makePerson("Robert", "Baratheon", "King of the Seven Kingdoms")

hail = (p :: Person) -> "Hails {p.name} of House {p.house}, {p.title}!"

addAll = (xs :: ...Number) -> xs

xs = addAll(1, 2, 3)

print(xs.length())

print(hail(robert))

