ListHead = [T] => type:
	value: T
	rest: List[T]

List = [T] => ListHead[T]?

s = "Hello"

listOf = [T] -> (arr: Array[T]) ->
	i = arr.length()
	list: List[T] = nothing
	while i > 0 do
		set i = i - 1
		list = ListHead[T](arr.at(i), list)
	list

# Rewrite symbol in scope with nothing check
mkString = [T] -> (originalList: List[T]) ->
	list = originalList
	string = ""
	while list != nothing do
		print("Hey")
		comma = if string == "", "" else ", "
		set string = "{string}{comma}{list.value}"
		set list = list.rest
	string

head = listOf[Number](1, 2, 3)
r = mkString[String](head)
print(r)
