# import collections/Iterable

Address = struct:
	street: String
	number: Number

Pet = struct:
	name: String

Person = struct:
	name: String
	address: Address
	pet: Pet?

AnimalLover = Person where:
	self.pet !: Nothing

person = Person { "Razvan", Address { "Iancu Avramescu", 10 } }

p2 = person { address.street = "Hi", pet = Pet { "Zapacel" } }
print(p2 :: AnimalLover)
