import collections/Iterable

Box = data:
	get: () -> ~Number
	setValue: (Number) ~> Void

ImmutableHolder = data:
	box: ~Box

makeBox = (n: Number) ->
	number: ~Number = n
	
	get = () ->
		~number 

	setValue = (newValue: Number) ~>
		set number = newValue

	Box(get, setValue)

ImmutableHolder(makeBox(2))
print("Hello")